{
  "name": "mmx-ui",
  "version": "0.0.1",
  "description": "demo",
  "type": "module",
  "main": "index.js",
  "author": "zjw",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zjwmmx/mmx-ui"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run clean-dist && rollup --config rollup.config.js --watch",
    "build-css": "gulp buildCSS",
    "build": "npm run clean-dist && rollup --config rollup.config.js",
    "dev:docs": "pnpm --filter doc run docs:dev",
    "dev:server": "pnpm --filter server run start:dev",
    "docs:preview": "pnpm --filter doc run docs:preview",
    "preview": "rollup preview",
    "clean-dist": "rm -rf packages/*/dist",
    "publish": "pnpm -r publish --access public"
  },
  "dependencies": {
    "@app/base": "workspace:^1.0.0",
    "@app/component": "workspace:^1.0.0",
    "@app/core": "workspace:^1.0.0",
    "axios": "^1.5.0",
    "change-case": "^4.1.2",
    "gulp": "^4.0.2",
    "gulp-sass": "^5.1.0",
    "lodash": "^4.17.21",
    "postcss-modules": "^6.0.0",
    "rollup-plugin-scss": "^4.0.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "@vue/babel-preset-jsx": "^1.4.0",
    "autoprefixer": "^10.4.16",
    "fs-jetpack": "^5.1.0",
    "glob": "^10.3.3",
    "rollup": "^3.27.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.66.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.8",
    "vue-tsc": "^1.8.5"
  }
}
